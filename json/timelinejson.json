{
  "$jason": {
    "head": {
      "title": "nCoV Tracker",
      "description": "This app track the Corona Virus (nCoV) stats",
	        "actions": {
        "$pull": {
          "trigger": "reload"
        },
        "$show": {
          "trigger": "reload"
        },
        "reload": {
          "type": "$network.request",
          "options": {
            "url": "https://tamimi-dev.github.io/app/timeline.html",
            "dataType": "html"
          },
          "success": {
            "type": "$render",
            "options": {
              "type": "html"
            },
            "success": {
              "type": "$util.toast",
              "options": {
                "text": "Refreshed"
              }
            }
          }
        }
      }
    },
    "body": {
      "header": {
        "title": "nCoV Tracker",
		"style": {
              "background": "rgb(31, 31, 31)",
              "theme": "dark",
              "color": "#fd304c"
			  },
	     "menu": {
              "text": "Contact Me",
              "href": {
                "url": "https://twitter.com/tamimi0x01",
                "view": "web"
              }
            }
      },
      "background": {
        "type": "html",
        "url": "https://tamimi-dev.github.io/app/timeline.html",
        "action": {
          "type": "$default"
        }
      },
	  
	  "footer": {
        "tabs": {
          "style": {
            "background": "rgb(31, 31, 31, 0.8)",
          },
          "items": [{
            "image": "https://raw.githubusercontent.com/tamimi-dev/app/master/assets/icons/Group.png",
            "style": {
              "height": "21"
            },
            "url": "https://raw.githubusercontent.com/tamimi-dev/app/master/json/timelinejson.json"
          }, {
            "image": "https://raw.githubusercontent.com/tamimi-dev/app/master/assets/icons/Group.png",
            "style": {
              "height": "21"
            },
            "url": "https://raw.githubusercontent.com/tamimi-dev/app/master/json/timelinejson.json"
          }, {
			"image": "https://raw.githubusercontent.com/tamimi-dev/app/master/assets/icons/Group.png",
            "style": {
              "height": "21"
            },
            "url": "https://raw.githubusercontent.com/tamimi-dev/app/master/json/timelinejson.json"
          }]
        }
      }
    }
  }
}
